{
    "builds": [
      {
        "src": "House_price_prediction/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "House_price_prediction/wsgi.py"
      }
    ],
    "installCommand": "pip install --no-cache-dir -r requirements.txt"
}
  
